# URL de conexão do banco de dados (ex: PostgreSQL no Render)
# DATABASE_URL=postgres://user:password@host:port/database_name

# OU, para desenvolvimento local com MySQL
DB_USERNAME=root
DB_PASSWORD=sua_senha_local

# Segredo para gerar os tokens JWT (pode ser qualquer string longa e aleatória)
JWT_SECRET=seu_segredo_super_secreto_aqui

# Origem que o backend aceitará requisições (URL do frontend)
CORS_ORIGIN=http://localhost:3000

# Token de API para o serviço Gemini do Google AI
GEMINI_TOKEN=sua_chave_de_api_do_gemini